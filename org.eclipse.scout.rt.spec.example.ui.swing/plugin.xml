<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="specApplication"
         name="specApplication"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.scout.rt.spec.example.ui.swing.app.SwingApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.scout.rt.spec.example.ui.swing.specApplication"
            name="Specification Example Dev Product">
         <property
               name="appName"
               value="Specification Example Dev Product">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.scout.service.services">
      <service
            class="org.eclipse.scout.rt.spec.example.ui.swing.text.SpecTextProviderTestService"
            createImmediately="false"
            ranking="20">
      </service>
   </extension>

</plugin>
